{
  "name": "shifter",
  "version": "0.3.0",
  "description": "A simple, self-hosted file-sharing web app, powered by Django.",
  "scripts": {
    "tailwind-dev": "npx tailwindcss -i /app/shifter/assets/css/style.css -o /app/shifter/static/css/style.css --watch",
    "tailwind-build": "npx tailwindcss -i /app/shifter/assets/css/style.css -o /app/shifter/static/css/style.css --minify",
    "js-dev": "webpack --mode development --watch",
    "js-build": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TobySuch/Shifter.git"
  },
  "author": "TobySuch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TobySuch/Shifter/issues"
  },
  "homepage": "https://github.com/TobySuch/Shifter#readme",
  "devDependencies": {
    "css-loader": "^6.10.0",
    "source-map-loader": "^5.0.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "filepond": "^4.30.6",
    "filepond-plugin-file-validate-size": "^2.2.8",
    "flowbite": "^2.3.0",
    "jszip": "^3.10.1"
  }
}
